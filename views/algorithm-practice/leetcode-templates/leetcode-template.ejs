<!DOCTYPE html>
<html lang = "en">
    <%- include('../../partials/page/head.ejs'); %>
    <body class = "fixed-header">
        <%- include('../../partials/page/header.ejs'); %>
        <%- include('../../partials/page/search.ejs'); %>
        <div id = "wrapper">

            <section class = "border-bottom-1 border-grey-300 padding-top-10 padding-bottom-10">
                <div class = "container">
                    <div class = "row">
                        <div class = "col-lg-12">
                            <ol class = "breadcrumb">
                                <li><a href = "/">Home</a></li>
                                <li><a href = "/algorithm-practice/leetcode">Leetcode</a>
                                </li>
                                <li class = "active"><%= title %></li>
                            </ol>
                        </div>
                    </div>
                </div>
            </section>


            <section>
                <div class = "container">
                    <div class = "panel panel-asphalt">
                        <div class = "panel-heading">
                            <h1 class = "panel-title" style = "font-size: 20px; vertical-align: middle">
                                <b><%= number %>. <%= title %></b></h1>
                            <span class = "panel-title question-information">Acceptances: <strong><%= rate %></strong></span>
                            <span class = "panel-title question-information">Difficulty: <strong><%= difficulty %></strong></span>

                        </div>
                        <div class = "panel-body">
                            <h3 class = "panel-title"><b>Question</b></h3>

                            <%- include('../leetcode-problems/problem-'+ href + '.ejs'); %>
                        </div>
                    </div>

                    <div class = "solution">
                        <% for (var i = 0; i < count; i++) { %>
                        <div class = "panel panel-green-sea">
                            <div class = "panel-heading">
                                <h1 style = "font-size: 20px" class = "panel-title">
                                    <b>Solution <%= i + 1 %></b></h1>
                            </div>
                            <div class = "panel-body">
                                <div>
                                    <h3 class = "panel-title split-para">
                                        <b>Code</b><span class = "text-info">Language: <strong>Java</strong></span>
                                    </h3>
                                    <pre class = "line-numbers language-java">
                                    <code class = "language-java">
                                        <%= include('../leetcode-java/java-'+ href + '_' + (i + 1) +'.java'); %>
                                    </code>
                                    </pre>
                                </div>
                                <div style = "margin-top: 20px">
                                    <h3 class = "panel-title split-para">
                                        <b>Explanation</b>

                                        <p>Explanation goes here...</p>

                                </div>
                            </div>
                        </div>
                        <% } %>
                    </div>
                </div>


            </section>


        </div><%- include('../../partials/page/footer.ejs'); %>
        <%- include('../../partials/page/script.ejs'); %>
    </body>
</html>